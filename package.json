    "three": "^0.160.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^18.19.39",
    "typescript": "^5.3.3",
    "vite": "^5.0.8"
